{
  "name": "c18-16-n-node-react",
  "version": "1.0.0",
  "description": "launch application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/No-Country/c18-16-n-node-react.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "scripts": {
    "install:all": "npm-run-all --parallel install:frontend install:backend",
    "install:frontend": "npm install --prefix frontend",
    "install:backend": "npm install --prefix backend",
    "prisma:generate": "npm run prisma:generate --prefix backend",
    "dev": "npm-run-all dev:prepare dev:backend dev:frontend",
    "dev:backend": "npm run dev --prefix backend",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:prepare": "npm run prisma:generate"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
